    #feedback_slideshow(style='display:none')
    script.
      var feedback_pos = 0;
      setInterval(() => {
      $("#feedback .feedback-wrap img").each(function (index) {
      $(this).css("width", $(window).width() * 0.5);
      var xtrans = $(window).width() * 0.15;
      if (index - feedback_pos == 1) { //right
      $(this).css("transform", "translateX(" + xtrans + "px) translateZ(-150px)");
      $(this).css("opacity", 0.5);
      } else if (feedback_pos - index == 1) {
      $(this).css("transform", "translateX(-" + xtrans + "px) translateZ(-150px)");
      $(this).css("opacity", 0.5);
      } else if (index == feedback_pos) {
      $(this).css("transform", "");
      $(this).css("opacity", 1);
      } else {
      $(this).css("opacity", 0);
      }
      if (index == feedback_pos) {
      $(this).css("z-index", 99);
      } else {
      $(this).css("z-index", 2);
      }
      //console.log(index);
      });
      $("#feedback .feedback-wrap").width($(".feedback-child").width());
      $("#feedback .feedback-wrap").height($(".feedback-child").height());
      }, 100);
      $("#feedback_slideshow").slideshow(["im/R1.jpg", "im/R2.jpg", "im/R3.jpg", "im/R4.jpg"], 1000);
      function feedbackResize() {
      if ($(window).width()<1280) {
      $("#feedback_slideshow").show();
      $(".feedback-left").hide();
      $(".feedback-wrap").hide();
      $(".feedback-right").hide();
      } else {
      $("#feedback_slideshow").hide();
      $(".feedback-left").show();
      $(".feedback-wrap").show();
      $(".feedback-right").show();
      }
      }feedbackResize();
      $(window).on("resize",feedbackResize);
